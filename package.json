{
  "name": "social-stocks",
  "version": "1.0.0",
  "description": "social-stocks",
  "homepage": "#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/tberey/social-stocks"
  },
  "main": "build/main.js",
  "types": "build/main.d.ts",
  "scripts": {
    "dev": "nodemon",
    "setup": "npm update && npm install",
    "cleanup:full": "rimraf ./logs && rimraf ./build",
    "cleanup:build": "rimraf ./build",
    "build": "npm run cleanup:full && npm run compile",
    "compile": "tsc",
    "quick:api": "node build/main.js",
    "quick:app": "electron build/main.js",
    "start:api": "npm run build && node build/main.js",
    "start:app": "npm run build && electron build/main.js",
    "upinstart": "npm update && npm install && npm run start:app",
    "test:app": "electron-mocha -r ts-node/register src/tests/*.test.ts",
    "test:api": "mocha --exit -r ts-node/register src/tests/deployment.test.ts",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' electron-mocha -r ts-node/register 'src/tests/*.ts'",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.2",
    "@types/mysql": "^2.15.18",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "electron": "^15.5.5",
    "eslint": "^7.29.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/chart.js": "^2.9.32",
    "@types/json2csv": "^5.0.2",
    "aws-sdk": "^2.946.0",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "chart.js": "^3.3.2",
    "dotenv": "^9.0.2",
    "ejs": "^3.1.6",
    "ejs-electron": "^2.1.1",
    "electron-mocha": "^10.0.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "json2csv": "^5.0.6",
    "mocha": "^8.4.0",
    "mysql": "^2.18.1",
    "request": "^2.88.2",
    "rollbar": "^2.21.1",
    "simple-txt-logger": "^1.0.5"
  }
}
